{
  "name": "@monorepo/be-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@monorepo/be"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/be-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@monorepo/be:build",
          "@monorepo/be:serve"
        ]
      }
    }
  }
}
